<!DOCTYPE html>
<html>
  <head>
    <title>Rorcap</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
<h1>Env :</h1>
  <% request.env.each do |key, value| %>
    <strong><%= key %></strong> => <%= value %><br/>
<% end %>
  <body>
    <%= yield %>
  </body>
</html>
